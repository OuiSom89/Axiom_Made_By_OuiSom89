function Discord()
	setclipboard("https://discord.gg/MsV8zJrZKe")
end

local function CMDEnter()
    print(CMDInsert.Text)
    local TTEnter = CMDInsert.Text
    if TTEnter == "Physics gun" then
        AddCommand("Add physique gun")
        Physique_gun()
    elseif TTEnter == "Fly" then
		Fly()
    elseif TTEnter == "Help" then
        AddHelpCommand("Hello I'm IA support.")
        AddHelpCommand("These are the keywords I can answer, otherwise")
        AddHelpCommand("I invite you to join the discord to solve")
        AddHelpCommand("your problem.")
        AddHelpCommand("Cmds, Commands, Discord")
	elseif TTEnter == "Cmds" or TTEnter == "Commands"then
		AddCommand("All commands are printed in consol \"F9\"")
		print("Physics gun, Fly, Help, Cmds, Commands, Help, Discord")
	elseif TTEnter == "Discord" then
		Discord()
		AddCommand("The link are in clipboard")
    else
        AllMessageConsol = AllMessageConsol + 1
        CMDConsoleMessage = Instance.new("TextLabel", CMDScrollingFrame)
        CMDConsoleMessage.BorderSizePixel = 0
        CMDConsoleMessage.Position = UDim2.new(0, 0, 0, 20*AllMessageConsol*ClientY)
        CMDConsoleMessage.Size = UDim2.new(0, 544*ClientX, 0, 20*ClientY)
        CMDConsoleMessage.Font = Enum.Font.GothamBold
        CMDConsoleMessage.Text = "  [ CMD ] \'"..CMDInsert.Text.."\' does not exist"      
        CMDConsoleMessage.TextScaled = true
        CMDConsoleMessage.TextColor3 = Color3.fromRGB(255, 127, 4)
        CMDConsoleMessage.TextXAlignment = Enum.TextXAlignment.Left
        CMDScrollingFrame.CanvasSize = UDim2.new(0, 544*ClientX, 0, 20*(AllMessageConsol+1)*ClientY)
    end
    CMDInsert.Text = ''
end

--[[]]--
--[[conextion logique]]--
--[[]]--

MenuButton.InputBegan:connect(function(InputBegan)
    if InputBegan.UserInputType == Enum.UserInputType.MouseButton1 then
        if pp == true then
            if p == true then
                if _ConfigScript.AnimationBolean then
                    pp = false
                    p = false
                    repeat
                        MenuHomeButton2.TextTransparency, MenuKeyButton2.TextTransparency, MenuScriptButton2.TextTransparency, MenuCMDButton2.TextTransparency, MenuOptionButton2.TextTransparency, MenuCreditsButton2.TextTransparency = MenuCreditsButton2.TextTransparency + 0.2, MenuHomeButton2.TextTransparency + 0.2, MenuKeyButton2.TextTransparency + 0.2, MenuKeyButton2.TextTransparency + 0.2, MenuKeyButton2.TextTransparency + 0.2, MenuKeyButton2.TextTransparency + 0.2
                        wait(0.1)
                    until(MenuHomeButton2.TextTransparency >= 1)
                    MenuHomeButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    MenuKeyButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    MenuScriptButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    MenuCMDButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    MenuOptionButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    MenuCreditsButton2:TweenSize(UDim2.new(0, 0*ClientX, 0, 26*ClientY))
                    wait(0.3)
                    MenuHomeButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    MenuKeyButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    MenuScriptButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    MenuCMDButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    MenuOptionButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    MenuCreditsButton:TweenSize(UDim2.new(0, 0*ClientX, 0, 30*ClientY))
                    wait(1)
                    MenuBis:TweenSize(UDim2.new(0, 0*ClientX, 0, 340*ClientY))
                    wait(0.9)
                    MenuBis2.Size = UDim2.new(0, 0*ClientX, 0, 6*ClientY)
                    Menu.Size = UDim2.new(0, 0*ClientX, 0, 340*ClientY)
                    wait(0.1)
                    Menu.Visible = false
                    pp = true
                else
                    MenuHomeButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuKeyButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuScriptButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuCMDButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuOptionButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuCreditsButton2.Size = UDim2.new(0, 0*ClientX, 0, 26*ClientY)
                    MenuHomeButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuKeyButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuScriptButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuCMDButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuOptionButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuCreditsButton.Size = UDim2.new(0, 0*ClientX, 0, 30*ClientY)
                    MenuBis.Size = UDim2.new(0, 0*ClientX, 0, 340*ClientY)
                    MenuBis2.Size = UDim2.new(0, 0*ClientX, 0, 6*ClientY)
                    Menu.Size = UDim2.new(0, 0*ClientX, 0, 340*ClientY)
                    Menu.Visible = false
                    MenuHomeButton2.TextTransparency, MenuKeyButton2.TextTransparency, MenuScriptButton2.TextTransparency, MenuCMDButton2.TextTransparency, MenuOptionButton2.TextTransparency, MenuCreditsButton2.TextTransparency = 1,1,1,1,1,1
                    pp = true
                    p = false
                end
            else
                if _ConfigScript.AnimationBolean then
                    pp = false
                    p = true
                    Menu.Visible = true
                    MenuBis2.Size = UDim2.new(0, 12*ClientX, 0, 6*ClientY)
                    Menu.Size = UDim2.new(0, 12*ClientX, 0, 340*ClientY)
                    MenuBis:TweenSize(UDim2.new(0, 266*ClientX, 0, 340*ClientY))
                    wait(1)
                    MenuHomeButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    MenuKeyButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    MenuScriptButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    MenuCMDButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    MenuOptionButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    MenuCreditsButton:TweenSize(UDim2.new(0, 250*ClientX, 0, 30*ClientY))
                    wait(0.3)
                    MenuHomeButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    MenuKeyButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    MenuScriptButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    MenuCMDButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    MenuOptionButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    MenuCreditsButton2:TweenSize(UDim2.new(0, 246*ClientX, 0, 26*ClientY))
                    wait(1)
                    repeat
                        MenuHomeButton2.TextTransparency, MenuKeyButton2.TextTransparency, MenuScriptButton2.TextTransparency, MenuCMDButton2.TextTransparency, MenuOptionButton2.TextTransparency, MenuCreditsButton2.TextTransparency = MenuCreditsButton2.TextTransparency - 0.2, MenuHomeButton2.TextTransparency - 0.2, MenuKeyButton2.TextTransparency - 0.2, MenuKeyButton2.TextTransparency - 0.2, MenuKeyButton2.TextTransparency - 0.2, MenuKeyButton2.TextTransparency - 0.2
                        wait(0.1)
                    until(MenuHomeButton2.TextTransparency <= 0)
                    pp = true
                else
                    MenuHomeButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuKeyButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuScriptButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuCMDButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuOptionButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuCreditsButton2.Size = UDim2.new(0, 246*ClientX, 0, 26*ClientY)
                    MenuHomeButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuKeyButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuScriptButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuCMDButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuOptionButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuCreditsButton.Size = UDim2.new(0, 250*ClientX, 0, 30*ClientY)
                    MenuBis.Size = UDim2.new(0, 266*ClientX, 0, 340*ClientY)
                    MenuBis2.Size = UDim2.new(0, 12*ClientX, 0, 6*ClientY)
                    Menu.Size = UDim2.new(0, 12*ClientX, 0, 340*ClientY)
                    Menu.Visible = true
                    MenuHomeButton2.TextTransparency, MenuKeyButton2.TextTransparency, MenuScriptButton2.TextTransparency, MenuCMDButton2.TextTransparency, MenuOptionButton2.TextTransparency, MenuCreditsButton2.TextTransparency = 0,0,0,0,0,0
                    pp = true
                    p = true
                end
            end
        end
    end
end)

MenuHomeButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Home.Visible = true
        Key.Visible, Script.Visible, CMD.Visible, Option.Visible, Credits.Visible = false, false, false, false, false
    end
end)

MenuKeyButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Key.Visible = true
        Home.Visible, Script.Visible, CMD.Visible, Option.Visible, Credits.Visible = false, false, false, false, false
    end
end)

MenuScriptButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Script.Visible = true
        Home.Visible, Key.Visible, CMD.Visible, Option.Visible, Credits.Visible = false, false, false, false, false
    end
end)

MenuCMDButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        CMD.Visible= true
        Home.Visible, Key.Visible, Script.Visible, Option.Visible, Credits.Visible = false, false, false, false, false
    end
end)

MenuOptionButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Option.Visible = true
        Home.Visible, Key.Visible, Script.Visible, CMD.Visible, Credits.Visible = false, false, false, false, false
    end
end)

MenuCreditsButton2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        Credits.Visible = true
        Home.Visible, Key.Visible, Script.Visible, CMD.Visible, Option.Visible = false, false, false, false, false
    end
end)

CMDInsert.FocusLost:Connect(function(enterPressed)
    if enterPressed then
        CMDEnter()
    end
end)

OptionNewInstanceCloneBoeleanbis.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        if _ConfigScript.AnimationBolean == false then
            if OptionNewInstanceCloneBoeleanbisbis == false then
                _ConfigScript.AnimationBolean = true
                OptionNewInstanceCloneBoeleanbisbis = true
                OptionNewInstanceCloneBoeleanBoleleanbis:TweenPosition(UDim2.new(0, 43*ClientX, 0.5, -12*ClientY))
                wait(1)
                OptionNewInstanceCloneBoeleanbisbis = false
                lib.WriteConfig("Axiom_Script", _ConfigScript)
            end
        else
            if OptionNewInstanceCloneBoeleanbisbis == false then
                OptionNewInstanceCloneBoeleanBoleleanbis.Position = UDim2.new(0, 3*ClientX, 0.5, -12*ClientY)
                _ConfigScript.AnimationBolean = false
                lib.WriteConfig("Axiom_Script", _ConfigScript)
            end
        end
    end
end)

-- option color 

local function ColorThemeChoose(_fggColor)
	for _, v in pairs(game:GetService("CoreGui").ScreenGui:GetDescendants()) do
		local x = tostring(v)
		if x == "Frame" or x == "ImageLabel" or x == "TextLabel" or x == "TextBox" or x == "ScrollingFrame" then
			if v.BackgroundColor3 == Color3.new(0,0,0) or v.BackgroundColor3 == Color3.new(0.113725, 0.2, 0.329412) or v.BackgroundColor3 == Color3.new(1, 0, 0) then
			    if _fggColor == "Bleu" then
    				v.BackgroundColor3 = Color3.new(0.113725, 0.2, 0.329412)
			    elseif _fggColor == "Rouge" then
			        v.BackgroundColor3 = Color3.new(1, 0, 0)
				elseif _fggColor == "Noir" then
			        v.BackgroundColor3 = Color3.new(0, 0, 0)
			    end
		    elseif v.BackgroundColor3 == Color3.new(0.235294,0.235294,0.235294) or v.BackgroundColor3 == Color3.new(0.101961,0.243137,0.411765) or v.BackgroundColor3 == Color3.new(1, 0, 0) then
				if _fggColor == "Bleu" then
    				v.BackgroundColor3 = Color3.new(0.101961,0.243137,0.411765)
			    elseif _fggColor == "Rouge" then
			        v.BackgroundColor3 = Color3.new(1, 0, 0)
				elseif _fggColor == "Noir" then
			        v.BackgroundColor3 = Color3.new(0.235294,0.235294,0.235294)
			    end
			elseif v.BackgroundColor3 == Color3.new(0.117647,0.117647,0.117647) or v.BackgroundColor3 == Color3.new(0.105882,0.164704,0.290196) or v.BackgroundColor3 == Color3.new(1, 0, 0) then
				if _fggColor == "Bleu" then
    				v.BackgroundColor3 = Color3.new(0.105882,0.164704,0.290196)
			    elseif _fggColor == "Rouge" then
			        v.BackgroundColor3 = Color3.new(1, 0, 0)
				elseif _fggColor == "Noir" then
			        v.BackgroundColor3 = Color3.new(0.117647,0.117647,0.117647)
			    end
			end
		end
	end
end

OptionColorThemeBleu2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	    local _fggColor = "Bleu"
	    ColorThemeChoose(_fggColor)
	end
end)

OptionColorThemeRouge2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	    local _fggColor = "Rouge"
	    ColorThemeChoose(_fggColor)
	end
end)

OptionColorThemeNoir2.InputBegan:connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	    local _fggColor = "Noir"
	    ColorThemeChoose(_fggColor)
	end
end)

