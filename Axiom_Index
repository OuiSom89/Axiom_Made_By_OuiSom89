if _ConfigScript.AnimationBolean then
    FirstGUI:TweenSize(UDim2.new(0, 550*ClientX, 0, 380*ClientY))
    wait(1)
    FirstGUI2:TweenSize(UDim2.new(0, 544*ClientX, 0, 374*ClientY))
    wait(1)
    repeat
        Titre.Transparency = Titre.Transparency - 0.1
        MenuButton.Transparency = MenuButton.Transparency - 0.1
        FirstGUI3.Transparency = FirstGUI3.Transparency - 0.1
        MenuButton.ImageTransparency = MenuButton.ImageTransparency - 0.1
        wait(0.1)
    until(Titre.TextTransparency <= 0)
    Home.Visible = true
else
    FirstGUI.Size = UDim2.new(0, 550*ClientX, 0, 380*ClientY)
    FirstGUI2.Size = UDim2.new(0, 544*ClientX, 0, 374*ClientY)
    Titre.Transparency, MenuButton.Transparency, FirstGUI3.Transparency, MenuButton.ImageTransparency, Home.Visible = true,0,0,0,0
end
